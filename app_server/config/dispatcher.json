{
    "MODULES" : {
	"service.projects.Projects" : "config/projects.json",
	"service.projects.Admin" : "config/projects_admin.json"
    },

    "HANDLERS" : {
	"POST" : {
	    "/projects/admin/login" : { "method" : "service.projects.Admin.login",
					"parameters" : ["username", "password"] },
	    "/projects/admin/logout" : { "method" : "service.projects.Admin.logout",
					 "parameters" : ["token"] },
	    "/projects/admin/adduser" : { "method" : "service.projects.Admin.add_user",
					  "parameters" : ["token", "username", "password", "name", "surname", "email"] },
	    "/projects/admin/deluser" : { "method" : "service.projects.Admin.del_user",
					  "parameters" : ["token", "username"] },
	    "/projects/admin/userinfo" : { "method" : "service.projects.Admin.get_uinfo",
					   "parameters" : ["username", "token"] },
	    "/projects/admin/updateuser" : { "method" : "service.projects.Admin.update_user",
					     "parameters" : ["username", "token"] },
	    "/projects/admin/loadusers" : { "method" : "service.projects.Admin.get_users",
					    "parameters" : ["token"] },
	    
	    "/projects/login" : { "method" : "service.projects.Projects.login",
				  "parameters" : ["username", "password"] },
	    "/projects/logout" : { "method" : "service.projects.Projects.logout",
				   "parameters" : ["token"] },
	    "/projects/listcategories" : { "method" : "service.projects.Projects.list_categories",
					   "parameters" : ["token"] },
	    "/projects/createproject" : { "method" : "service.projects.Projects.create_project",
					  "parameters" : ["token", "projectname", "category"] },
	    "/projects/listprojects" : { "method" : "service.projects.Projects.list_projects",
					 "parameters" : ["token"] },
	    "/projects/loadproject" : { "method" : "service.projects.Projects.load_project",
					"parameters" : ["token", "projectid"] },
	    "/projects/uploadaudio" : { "method" : "service.projects.Projects.upload_audio",
					"parameters" : ["token", "projectid", "filename", "file"] },
	    "/projects/saveproject" : { "method" : "service.projects.Projects.save_project",
					"parameters" : ["token", "projectid", "tasks"] },
	    "/projects/diarizeaudio" : { "method" : "service.projects.Projects.diarize_audio",
					 "parameters" : ["token", "projectid"] }
	},
	
    	"GET" : {
    	    "/projects/projectaudio" : { "method" : "service.projects.Projects.project_audio",
					 "parameters" : ["token", "projectid"] }
	},

    	"PUT" : {}
    },

    "TEMPIO_MODULES": ["service.projects.Projects"],
    
    "logging" : {
	"dir" : "/mnt/stp/",
	"format" : "%(asctime)s :: %(name)s :: %(levelname)s :: %(message)s"
    }
    
}
