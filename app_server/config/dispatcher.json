{
    "MODULES" : {
	"service.auth.Auth" : "config/auth.json",
	"service.admin.Admin" : "config/admin.json"
    },

    "HANDLERS" : {
	"PUT" : {
	    "/addtoken" : { "method" : "service.auth.Auth.addtoken",
			    "parameters" : ["name"] },
	    "/adduser" : { "method" : "service.admin.Admin.adduser",
			    "parameters" : ["name"] }
	},
	"GET" : {
	    "/checktoken" : { "method" : "service.auth.Auth.checktoken", 
			      "parameters" : ["name"] }
	}
    },

    "logging" : {
	"dir" : "logging/",
	"format" : "%(asctime)s :: %(name)s :: %(levelname)s :: %(message)s"
    }
}
